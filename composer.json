{
    "name": "DeividKaiser/CidadessBR-Codeigniter",
    "type": "library",
    "description": "tabelas estados e cidades brasileiras para CodeIgniter 4.",
    "keywords": [
        "codeigniter",
        "Cidades",
        "Estados"
    ],
    "homepage": "https://github.com/Cidades-Estados-Brasileiros-Codeigniter",
    "license": "MIT",
    "authors": [{
        "name": "Deivid Kaiser",
        "email": "deividkaiser@gmail.com",
        "homepage": "https://github.com/DeividKaiser",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || 7.3 || ^8.0"
    },

    "require-dev": {
        "codeigniter4/codeigniter4": "dev-develop",
        "codeigniter4/codeigniter4-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DeividKaiser\\CidadessBR-Codeigniter\\": "src"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },

    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/codeigniter4/CodeIgniter4"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true
}